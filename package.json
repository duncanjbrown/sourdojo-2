{
  "name": "sourdojo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "shadow-cljs clj-run sourdojo.build/release local",
    "clean": "rm -rf dist/js",
    "watch": "shadow-cljs clj-run sourdojo.build/watch",
    "clear-cloudfront-cache": "aws cloudfront create-invalidation --profile sourdojo --distribution-id EL31UBHNJMT8Q --paths \"/index.html\" \"/style.css\"",
    "deploy": "aws --profile sourdojo s3 sync --delete ./dist s3://sourdojo && npm run clear-cloudfront-cache",
    "cljfmt": "clojure -Sdeps '{:deps {cljfmt {:mvn/version \"0.6.4\"}}}' -m cljfmt.main fix src/sourdojo",
    "lint": "clj-kondo --lint \"src\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "create-react-class": "^15.6.3",
    "firebase": "^7.13.2",
    "highlight.js": "9.18.1",
    "process": "^0.11.10",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-flip-move": "^3.0.4",
    "react-highlight.js": "1.0.7"
  }
}
